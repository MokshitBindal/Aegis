{
  "name": "aegis-siem-server",
  "version": "1.0.0",
  "description": "Aegis SIEM Backend Server",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.13"
  }
}
